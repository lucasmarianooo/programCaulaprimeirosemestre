#include <stdio.h>
#include <stdlib.h>

int main ()
{
	float nota1, nota2, nota3, media;
	printf("Digite nota1: ");
	scanf("%f, &nota1");
	printf("Digite nota2: ");
	scanf("%f, &nota2");
	printf("Digite nota3: ");
	scanf("%f, &nota3: ");
	
	media = (nota1 + nota2 + nota3) / 3;
	
	printf ("A media e %.2f, media");
	
	return 0;
}
